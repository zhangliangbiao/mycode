@Service("houseExplorationServiceFacade")
public class HouseExplorationServiceFacade implements HouseExplorationService {

    @Autowired
    private HouseExplorationServiceBiz houseExplorationServiceBiz;

	@Override
	public ResultMsg saveHouseExploration(HouseExploration houseExploration,
			SessionVo sessionVo) {
		return houseExplorationServiceBiz.saveHouseExploration(houseExploration,sessionVo);
	}

	@Override
	public ResultMsg saveHouseExplorationImg(
			HouseExplorationImg houseExplorationImg, SessionVo session) {
		return houseExplorationServiceBiz.saveHouseExplorationImg(houseExplorationImg,session);
	}

	@Override
	public ResultObjectMsg<PagingHelper<HouseExploration>> searchHouseExplorationList(Map<String, Object> pageData,
			SessionVo sessions) {
		return houseExplorationServiceBiz.searchHouseExplorationList(pageData,sessions);
	}

	@Override
	public ResultObjectMsg<List<HouseExplorationImg>> selectHouseExplorationImgList(Map<String, Object> pageData,
			SessionVo sessions) {
		return houseExplorationServiceBiz.selectHouseExplorationImgList(pageData,
				 sessions);
	}
	
	@Override
	public ResultMsg deleteHouseExploration(Map<String, Object> pageData){
		return houseExplorationServiceBiz.deleteHouseExploration(pageData);
	}
	
	@Override
	public ResultMsg auditHouseExploration(Map<String, Object> pageData){
		return houseExplorationServiceBiz.auditHouseExploration(pageData);
	}
}
