package com.isz.erp.house.service.impl;

import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.isz.erp.common.page.PagingHelper;
import com.isz.erp.common.util.IszLogger;
import com.isz.erp.common.util.ResultObjectMsg;
import com.isz.erp.facade.house.entity.ApartmentCount;
import com.isz.erp.facade.house.entity.ApartmentCountRented;
import com.isz.erp.facade.house.service.ApartmentCountService;
import com.isz.erp.house.biz.ApartmentCountServiceBiz;

@Service("apartmentCountServiceFacade")
public class ApartmentCountServiceFacade implements ApartmentCountService {
    @Autowired
    ApartmentCountServiceBiz buryingPointServiceBiz;

	@Override
	public ResultObjectMsg<PagingHelper<ApartmentCount>> searchApartmentCountList(Map<String, Object> pageData) {
		try {
			return buryingPointServiceBiz.searchApartmentCountList(pageData);
		} catch (Exception e) {
			IszLogger.error("BuryingPointServiceFacade.searchApartmentCountList", e);
			return new ResultObjectMsg<PagingHelper<ApartmentCount>>(e.getMessage());
		}
	}

	@Override
	public ResultObjectMsg<ApartmentCountRented> searchRentedDetail(Map<String, Object> pageData) {
		try {
			return buryingPointServiceBiz.searchRentedDetail(pageData);
		} catch (Exception e) {
			IszLogger.error("BuryingPointServiceFacade.searchRentedDetail", e);
			return new ResultObjectMsg<ApartmentCountRented>(e.getMessage());
		}
	}
}
